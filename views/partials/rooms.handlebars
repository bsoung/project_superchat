<div class="list-group">
	{{#each rooms as |room|}}
			{{#if room.active}}
				<a data-room-id="{{room.id}}" href="#" class="list-group-item active">{{room.roomname}}</a>
			{{else}}
				<a data-room-id="{{room.id}}" href="#" class="list-group-item">{{room.roomname}}</a>
			{{/if}}
	{{/each}}
</div>
